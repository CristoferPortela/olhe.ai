      "type": "integer",
      "required": true,
      "unique": true,
      "max": 9999,
      "min": 0,
      "private": true
    },
    "work_histories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::work-history.work-history",
      "mappedBy": "employer"
    }
  }
